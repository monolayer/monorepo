{
	"name": "@monorepo/push",
	"version": "0.0.0",
	"type": "module",
	"exports": {
		"./*": "./src/*"
	},
	"scripts": {
		"build": "tsc -b tsconfig.build.esm.json && tsc -b tsconfig.build.cjs.json",
		"bundle": "tsc -b tsconfig.bundle.esm.json && tsc -b tsconfig.bundle.cjs.json",
		"check": "npm run check:prettier && npm run check:lint && npm run check:skott",
		"check:lint": "npx eslint .",
		"check:prettier": "prettier --check .",
		"check:skott": "skott --displayMode=file-tree --showCircularDependencies --no-trackTypeOnlyDependencies --cwd src",
		"test": "NO_COLOR=1 vitest --run"
	},
	"devDependencies": {
		"@commander-js/extra-typings": "^12.1.0"
	},
	"dependencies": {
		"@vitest/coverage-istanbul": "^2.1.1",
		"@vitest/ui": "^2.1.1",
		"skott": "^0.33.2",
		"type-fest": "^4.10.1",
		"type-testing": "^0.2.0",
		"vitest": "^2.1.1",
		"@monorepo/cli": "*",
		"@monorepo/db": "*",
		"@monorepo/configuration": "*",
		"@monorepo/eslint-config": "*",
		"@monorepo/pg": "*",
		"@monorepo/programs": "*",
		"@monorepo/services": "*",
		"@monorepo/state": "*",
		"@monorepo/typescript-config": "*",
		"@monorepo/utils": "*",
		"@types/node": "^18.0.0",
		"case-anything": "^2.1.13",
		"kysely": "^0.27.3",
		"pg": "^8.11.3",
		"microdiff": "^1.3.2",
		"picocolors": "^1.0.0",
		"nunjucks": "^3.2.4",
		"ora": "^8.1.0",
		"sql-formatter": "^15.4.5",
		"indent-string": "^5.0.0",
		"prompts": "^2.4.2"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
